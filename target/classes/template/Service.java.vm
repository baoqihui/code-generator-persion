package ${package}.service.impl;

import ${package}.entity.model.${className};
import ${package}.service.api.I${className}Service;
import ${package}.service.repo.I${className}Repo;
import lombok.extern.slf4j.Slf4j;
import lombok.AllArgsConstructor;
import org.springframework.stereotype.Service;

/**
 * @Author: ${author}
 * @Description: ${comments}服务接口实现
 * @Date: ${datetime}
 */
@Slf4j
@Service
@AllArgsConstructor
public class ${className}Service implements I${className}Service {
    private I${className}Repo ${classname}Repository;

    @Override
    public ${className} getById(Long id) {
        return ${classname}Repository.findById(id).orElse(null);
    }
}

