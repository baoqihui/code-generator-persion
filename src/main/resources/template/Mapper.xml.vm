<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="${package}.service.repo.${className}Mapper">
    <select id="findList" resultType="${package}.entity.model.${className}">
        select
        #foreach ($column in $columns)
            t.$column.columnName $column.attrname,
        #end
        from ${tableName} t
        where 1=1
        #foreach ($column in $columns)
            #if( $column.columnName =="id")
            #elseif($column.columnName =="create_time")
            #elseif($column.columnName =="update_time")
            #else
            <if test="p.$column.attrname != null and p.$column.attrname != ''">
                and t.$column.columnName LIKE #[['%${p.]]#$column.attrname#[[}%']]#
            </if>
            #end
        #end
            <if test="p.startTime != null and p.startTime != '' ">
                and t.create_time >= '${p.startTime}'
            </if>
            <if test="p.endTime !=null and p.endTime != '' ">
                and '${p.endTime}' >= t.create_time
            </if>
        order by t.id desc
    </select>
</mapper>
